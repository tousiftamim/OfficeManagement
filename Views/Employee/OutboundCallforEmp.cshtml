@model System.Collections.Generic.IEnumerable<Office_Management.Models.OutboundCallforEmpVM>

@{
    ViewBag.Title = "OutboundCallforEmp";
}

<h2>My Outbound Call</h2>

@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table table-bordered table-stripped table-hover">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PhoneNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Reason)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.PhoneNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Reason)
        </td>
        <td>
            @Html.ActionLink("Comment", "CommentForOutboundcall", new {id=item.Id}, null)

@*            <a href="/Views/Employee/CommentforOutboundcall/@item.Id">comment</a>*@
        </td>
    </tr>
}

</table>
